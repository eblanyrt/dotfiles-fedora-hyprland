{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#10101090",
                "editor.background": "#10101090",
                "editor.foreground": "#c3c3c3",
                "text": "#c3c3c3",
                "text.muted": "#c3c3c370",
                "text.ignored": "#c3c3c340",
                "text.placeholder": "#c3c3c350",
                "ignored": "#c3c3c330",
                "element.hover": "#c3c3c330",
                "ghost_element.hover": "#566BAE30",
                "ghost_element.selected": "#566BAE30",
                "ghost_element.active": "#566BAE60",
                "border": "#101010",
                "editor.highlighted_line.background": "#566BAE10",
                "editor.active_line.background": "#566BAE10",
                "panel.background": "#10101090",
                "title_bar.background": "#10101090",
                "title_bar.inactive_background": "#10101090",
                "status_bar.background": "#10101090",
                "drop_target.background": "#10101090",
                "elevated_surface.background": "#101010",
                "toolbar.background": "#10101090",
                "tab_bar.background": "#10101090",
                "tab.inactive_background": "#10101090",
                "tab.active_background": "#566BAE30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#101010",
                "scrollbar.thumb.background": "#566BAE",
                "editor.gutter.background": "#10101090",
                "terminal.background": "#10101010",
                "terminal.foreground": "#c3c3c3",
                "terminal.dim_foreground": "#c3c3c3",
                "terminal.bright_foreground": "#c3c3c3",
                "terminal.ansi.black": "#101010",
                "terminal.ansi.red": "#566BAE",
                "terminal.ansi.green": "#49A4C1",
                "terminal.ansi.yellow": "#C6B3BE",
                "terminal.ansi.blue": "#AAE5D4",
                "terminal.ansi.magenta": "#FCD4CC",
                "terminal.ansi.cyan": "#CFCEF1",
                "terminal.ansi.white": "#c3c3c3",
                "terminal.ansi.bright_black": "#6c5959",
                "terminal.ansi.bright_red": "#566BAE",
                "terminal.ansi.bright_green": "#49A4C1",
                "terminal.ansi.bright_yellow": "#C6B3BE",
                "terminal.ansi.bright_blue": "#AAE5D4",
                "terminal.ansi.bright_magenta": "#FCD4CC",
                "terminal.ansi.bright_cyan": "#CFCEF1",
                "terminal.ansi.bright_white": "#c3c3c3",
                "modified": "#566BAE",
                "syntax": {
                    "attribute": {
                        "color": "#566BAE"
                    },
                    "boolean": {
                      "color": "#49A4C1"
                    },
                    "comment": {
                      "color": "#6c5959"
                    },
                    "comment.doc": {
                      "color": "#6c5959"
                    },
                    "constant": {
                      "color": "#49A4C1"
                    },
                    "function": {
                      "color": "#CFCEF1"
                    },
                    "keyword": {
                      "color": "#AAE5D4"
                    },
                    "number": {
                      "color": "#FCD4CC"
                    },
                    "operator": {
                      "color": "#AAE5D4"
                    },
                    "string": {
                      "color": "#566BAE"
                    },
                    "variable": {
                      "color": "#49A4C1"
                    }
                },
                "players": [
                  {
                    "cursor": "#566BAE",
                    "background": "#101010",
                    "selection": "#566BAE30"
                  }
                ]
            }
        }
    ]
}
